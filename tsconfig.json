{
  "extends": "expo/tsconfig.base",
  "include": ["src/**/*"],
  "exclude": [
    "node_modules/**/*"
  ],
  "compilerOptions": {
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "ES6",
  }
}
